<!DOCTYPE module PUBLIC "//gwt-module/" "http://google-web-toolkit.googlecode.com/svn/tags/1.6.2/distro-source/core/src/gwt-module.dtd">
<module rename-to="webclient">

      <!-- Inherit the core Web Toolkit stuff.                        -->
      <inherits name='com.google.gwt.user.User'/>

      <!--  inherit css based theme -->
      <inherits name='com.google.gwt.user.theme.standard.Standard'/>

      <!-- Specify the app entry point class.                         -->
      <entry-point class='org.vpac.grisu.client.Application'/>

      <!-- Specify the application specific style sheet.              -->
      <stylesheet src='Application.css' />
      <stylesheet src="js/ext/resources/css/ext-all.css"/>
      <script src="js/ext/adapter/ext/ext-base.js"/>
      <script src="js/ext/ext-all.js"/>
      <inherits name="com.gwtext.GwtExt"/>
      
      <inherits name="com.google.gwt.xml.XML" />
   
      <servlet class="org.vpac.grisu.server.GwtServiceInterfaceImpl" path="/Application/serviceInterface"/>

   
</module>


